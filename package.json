{
  "name": "hutz-ui",
  "version": "0.0.0",
  "description": "Hutz is UI component library built on React",
  "main": "lib/index.js",
  "author": "MIT",
  "homepage": "https://github.com/nicholaslmitchell/hutz-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicholaslmitchell/hutz-uti.git"
  },
  "bugs": {
    "url": "https://github.com/nicholaslmitchell/hutz/issues"
  },
  "dependencies": {
    "classnames": "^2.1.2",
    "history": "^1.14.0",
    "immutable": "^3.7.5",
    "radium": "^0.15.3",
    "ramda": "^0.18.0",
    "react": "^0.14.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^3.1.14",
    "babel-jest": "^6.0.1",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-plugin-transform-decorators": "^6.3.13",
    "babel-plugin-transform-decorators-legacy-back": "^1.1.0",
    "babel-plugin-transform-es2015-typeof-symbol": "^6.3.13",
    "babel-plugin-transform-object-assign": "^6.3.13",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-tape-runner": "^2.0.0",
    "babelify": "^7.2.0",
    "blue-tape": "^0.1.11",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "gulp": "^3.9.0",
    "gulp-jest": "^0.4.0",
    "harmonize": "^1.4.4",
    "jest-cli": "^0.8.1",
    "jsdom": "^7.2.1",
    "jsdomify": "^1.0.2",
    "karma": "^0.13.15",
    "onchange": "^2.0.0",
    "react-addons-test-utils": "^0.14.3",
    "react-component-gulp-tasks": "^0.7.0",
    "react-tools": "^0.13.3",
    "run-sequence": "^1.1.2",
    "skin-deep": "^0.13.0",
    "symbol": "^0.2.1",
    "tap-spec": "^4.1.1"
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "scripts": {
    "build": "gulp clean && NODE_ENV=production gulp build",
    "examples": "gulp dev:server",
    "lint": "eslint ./; true",
    "publish:site": "gulp publish:examples",
    "release": "gulp release",
    "start": "gulp dev",
    "test": "babel-tape-runner ./__tests__/*.js | tap-spec",
    "test:watch": "onchange 'src/**/*.js' '__tests__/*.js' -- npm test",
    "watch": "gulp watch:lib"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "jest": {
    "scriptPreprocessor": "<rootDir>/__tests__/support/preprocessor",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/[a-z]+"
    ]
  }
}
